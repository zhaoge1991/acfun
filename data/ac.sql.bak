SET NAMES UTF8;
DROP DATABASE IF EXISTS ac;
CREATE DATABASE ac CHARSET=UTF8;
USE ac;

CREATE TABLE ac_collect(
  cid INT PRIMARY KEY AUTO_INCREMENT,
  upname VARCHAR(64),
  cname VARCHAR(128),
  types VARCHAR(64),
  pic VARCHAR(128),
  pics VARCHAR(128),
  play  VARCHAR(32),
  comment VARCHAR(32),
  collects VARCHAR(32),
  detail VARCHAR(128)
);
